#include <chrono>
#include <thread>
#include <bits/stdc++.h>
// final boss of chess
 
using namespace std;
 
using ll = long long;
using vi = vector<int>;
using vl = vector<ll>;
using vvi = vector<vi>;
using vvl = vector<vl>;
 
#define pb push_back
#define sz size()
#define srt(a) sort(a.begin(), a.end())
#define revsrt(a) sort(a.rbegin(), a.rend())
 
#define fr(i, a, b) for (ll i = (a); i < (b); ++i)
#define fl(i, a) fr(i, 0, a)
 
void setIO(string s) {
	freopen((s + ".in").c_str(), "r", stdin);
	freopen((s + ".out").c_str(), "w", stdout);
}
void unsyncIO() {
	cin.tie(0)->sync_with_stdio(0);
}
 
 
void quick(string q) {
	cout << q << endl;
}
 
 
void solve(){	
	ll n, k, q, m;
	string s, s1, s2;
 
	cin >> s >> m >> s1 >> s2;
	n = s.length();
 
	set<char> st;
	int nxt = -1;
	for(int i = 0; i < m && nxt < n; i++){
		st.clear();
		fr(j, s1[i], s2[i]+1) st.insert(j);
 
		fr(j, nxt+1, n){
			if(st.find(s[j]) != st.end()){
				st.erase(s[j]);
				nxt = j;
			}
			if(st.size() == 0) break;
		}
		if(st.size()){
			cout << "YES\n";
			return;
		}
	}
 
	cout << "NO\n";
 
}
 
int main() {
	unsyncIO();
	// freopen("namenum.in", "r", stdin);
	// freopen("namenum.out", "w", stdout);
		
	ll t = 1;
	cin >> t;
	while(t--)
		solve();
}
